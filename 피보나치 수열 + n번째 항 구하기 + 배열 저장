#include <stdio.h>

void fib_arr(int *a, int n){
    a[0] = 0;
    a[1] = 1;
    for(int i=2;i<n;i++){
        a[i] = a[i-1] + a[i-2];
    }
}

int main(){
    int n = 20;
    int a[50];

    fib_arr(a, n);

    printf("Fib seq: ");
    for(int i=0;i<n;i++){
        printf("%d ", a[i]);
    }

    int k = 10;
    printf("\n%dth = %d", k, a[k-1]);
}
