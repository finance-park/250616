#include <stdio.h>

long fib_recursive(int n) {
    if (n <= 1)
        return n;
    return fib_recursive(n - 1) + fib_recursive(n - 2);
}

long fib_iterative(int n) {
    if (n <= 1)
        return n;
    long prev = 0, curr = 1, next;
    for (int i = 2; i <= n; i++) {
        next = prev + curr;
        prev = curr;
        curr = next;
    }
    return curr;
}

int main(void) {
    int n;

    printf("몇 번째 피보나치 수를 구할까요? (n ≥ 0) : ");
    if (scanf("%d", &n) != 1 || n < 0) {
        fprintf(stderr, "올바른 0 이상의 정수를 입력하세요.\n");
        return 1;
    }

    // --- 결과 출력 ---
    // 1) 재귀 함수 결과
    printf("fib_recursive(%d) = %ld\n", n, fib_recursive(n));

    // 2) 반복 함수 결과
    printf("fib_iterative(%d) = %ld\n", n, fib_iterative(n));

    // 3) 수열 전체(0부터 n까지) 출력 예시
    printf("\nFibonacci sequence up to %d:\n", n);
    for (int i = 0; i <= n; i++) {
        printf("%ld ", fib_iterative(i));
    }
    printf("\n");

    return 0;
}
